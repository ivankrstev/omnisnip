{
  "name": "@omnisnip/cli",
  "version": "1.0.0",
  "bin": {
    "omnisnip": "./dist/index.js"
  },
  "main": "dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
