{
  "name": "@omnisnip/types",
  "version": "0.1.0",
  "description": "Shared TypeScript types for OmniSnip",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
